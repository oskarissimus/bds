in := "$S3_TMP/tmp/in.txt"
out := "out.txt"
out2 := "out2.txt"

task(out <- in) {
    sys echo STRICT_REMOTE_DEPENDENCIES = $strictRemoteDependencies
    sys echo $in | tee $out
}

wait

task(out2 <- in, strictRemoteDependencies := true) {
    sys echo STRICT_REMOTE_DEPENDENCIES = $strictRemoteDependencies
    sys echo $in | tee $out2
}
