class SimpleException extends Exception {}

bool okCatch, okFinally

void func() {
    a := 42
	try {
		throw new SimpleException()
	} catch (SimpleException e) {
		println "Catch"
		okCatch = true
	} finally {
		println "Finaly"
		okFinally = true
	}
}

void testFoo() {
    t := 3.14
    debug "Enter debug mode"
    breakpoint "Break!"
	func()
	assert(okCatch)
	assert(okFinally)
}

testFoo()
